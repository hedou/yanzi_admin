<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.ssh.influence.domain.Influence" table="Influence" dynamic-insert="true" dynamic-update="true">

		<id name="InfluenceId" column="InfluenceId">
			<generator class="native"></generator>
		</id>

		<property name="num" column="num"></property>
		<property name="name" column="name"></property>
		<property name="StartTime" column="StartTime"></property>
		<property name="EndTime" column="EndTime"></property>
		
		<property name="Deletes" column="Deletes"></property>	
		<property name="Valid" column="Valid"></property>
		
		
	</class>
</hibernate-mapping>